<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
    <Pane prefHeight="600.0" prefWidth="800.0">
        <ImageView fx:id="leftEye" fitHeight="100.0" fitWidth="100.0" layoutX="250.0" layoutY="150.0" pickOnBounds="true" preserveRatio="true" onMouseClicked="#leftEyeAction">
        </ImageView>
        <ImageView fx:id="rightEye" fitHeight="100.0" fitWidth="100.0" layoutX="450.0" layoutY="150.0" pickOnBounds="true" preserveRatio="true" onMouseClicked="#rightEyeAction">
        </ImageView>
        <ImageView fx:id="nose" fitHeight="150.0" fitWidth="100.0" layoutX="350.0" layoutY="250.0" pickOnBounds="true" preserveRatio="true" onMouseClicked="#noseAction">
        </ImageView>
        <ImageView fx:id="mouth" fitHeight="75.0" fitWidth="225.0" layoutX="288.0" layoutY="400.0" pickOnBounds="true" preserveRatio="true" onMouseClicked="#mouthAction">
        </ImageView>
    </Pane>
</GridPane>
